    "test:security": "jest tests/security --coverage",
    "test:performance": "jest tests/performance --coverage",
    "test:reliability": "jest tests/reliability --coverage",
    "test:watch": "jest --watch",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security && npm run test:performance && npm run test:reliability",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"tests/**/*.js\"",
    "dev": "nodemon src/index.js",
    "start": "node src/index.js"
  },
